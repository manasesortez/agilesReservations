<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#11cfc5"
    tools:context=".admin.Doctor.AddDoctor">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/seguisb"
        android:text="Add Doctor"
        android:textColor="#fff"
        android:textSize="60px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.108"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.04"
        tools:ignore="HardcodedText,MissingConstraints,PxUsage,SpUsage" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/seguisb"
        android:text="Click to Add some Doctor"
        android:textColor="#fff"
        android:textSize="30px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.018"
        tools:ignore="HardcodedText,MissingConstraints,PxUsage" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="3"
        android:layout_marginTop="300px"
        android:background="@drawable/card_background"
        android:elevation="36dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="MissingConstraints,PxUsage,UnusedAttribute">


        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="39dp"
            android:adjustViewBounds="false"
            android:onClick="openAdminDoctor"
            android:src="@drawable/ic_back_blue"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="ContentDescription,OnClick,RtlHardcoded,UsingOnClickInXml" />


        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"

            android:layout_marginTop="0dp"
            android:layout_marginEnd="0dp"
            android:layout_marginRight="0dp"
            android:layout_marginBottom="0dp"
            tools:ignore="ObsoleteLayoutParam,RtlHardcoded,ScrollViewSize">

            <EditText
                android:id="@+id/txtNombreCompleto"
                android:layout_width="353dp"
                android:layout_height="40dp"
                android:layout_below="@+id/textView"
                android:background="@drawable/edit_text_background"
                android:drawableStart="@drawable/ic_baseline_person_24"
                android:drawablePadding="8dp"
                android:ems="10"
                android:fontFamily="@font/seguisb"
                android:hint="FullName"
                android:paddingLeft="15dp"
                android:textColor="#283241"
                android:textColorHint="#495057"
                android:textSize="17dp"

                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.083"
                tools:ignore="Autofill,HardcodedText,LabelFor,RtlCompat,RtlHardcoded,RtlSymmetry,SpUsage,TextFields,UnknownIdInLayout" />

            <EditText
                android:id="@+id/txtTelefono"
                android:layout_width="353dp"
                android:layout_height="40dp"
                android:layout_below="@+id/textView"
                android:background="@drawable/edit_text_background"
                android:drawableStart="@drawable/ic_baseline_address"
                android:drawablePadding="8dp"
                android:ems="10"
                android:fontFamily="@font/seguisb"
                android:hint="Address"
                android:inputType="phone"
                android:paddingLeft="15dp"
                android:textColor="#283241"
                android:textColorHint="#495057"
                android:textSize="17dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.173"
                tools:ignore="Autofill,HardcodedText,LabelFor,RtlCompat,RtlHardcoded,RtlSymmetry,SpUsage,TextFields,UnknownIdInLayout" />

            <EditText
                android:id="@+id/txtTelefono2"
                android:layout_width="353dp"
                android:layout_height="40dp"
                android:layout_below="@+id/textView"
                android:background="@drawable/edit_text_background"
                android:drawableStart="@drawable/ic_baseline_attach_money_24"
                android:drawablePadding="8dp"
                android:ems="10"
                android:fontFamily="@font/seguisb"
                android:hint="Consultation Cost"
                android:paddingLeft="15dp"
                android:textColor="#283241"
                android:textColorHint="#495057"
                android:textSize="17dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.268"
                tools:ignore="Autofill,HardcodedText,LabelFor,RtlCompat,RtlHardcoded,RtlSymmetry,SpUsage,TextFields,UnknownIdInLayout" />

            <EditText
                android:id="@+id/txtTelefono3"
                android:layout_width="353dp"
                android:layout_height="40dp"
                android:layout_below="@+id/textView"
                android:background="@drawable/edit_text_background"
                android:drawableStart="@drawable/ic_baseline_edit_calendar_24"
                android:drawablePadding="8dp"
                android:ems="10"
                android:fontFamily="@font/seguisb"
                android:hint="Business - Days"
                android:paddingLeft="15dp"
                android:textColor="#283241"
                android:textColorHint="#495057"
                android:textSize="17dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.363"
                tools:ignore="Autofill,HardcodedText,LabelFor,RtlCompat,RtlHardcoded,RtlSymmetry,SpUsage,TextFields,UnknownIdInLayout" />

            <TextView
                android:id="@+id/tv_horaEntrada"
                android:layout_width="353dp"
                android:layout_height="40dp"
                android:layout_below="@+id/textView"
                android:background="@drawable/edit_text_background"
                android:clickable="true"
                android:drawableStart="@drawable/ic_baseline_access_time_filled_24"
                android:drawablePadding="8dp"
                android:ems="10"
                android:fontFamily="@font/seguisb"
                android:hint="Entry Time"
                android:padding="5dp"
                android:paddingStart="15dp"
                android:paddingLeft="15dp"
                android:textColor="#283241"
                android:textColorHint="#495057"
                android:textSize="17dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.461"
                tools:ignore="Autofill,HardcodedText,KeyboardInaccessibleWidget,LabelFor,RtlCompat,RtlHardcoded,RtlSymmetry,SpUsage,UnknownIdInLayout,UseCompatTextViewDrawableXml" />

            <TextView
                android:id="@+id/tv_horaSalida"
                android:layout_width="353dp"
                android:layout_height="40dp"
                android:layout_below="@+id/textView"
                android:background="@drawable/edit_text_background"
                android:clickable="true"
                android:drawableStart="@drawable/ic_baseline_bedtime_24"
                android:drawablePadding="8dp"
                android:ems="10"
                android:fontFamily="@font/seguisb"
                android:hint="Departure Time"
                android:padding="5dp"
                android:paddingStart="15dp"
                android:paddingLeft="15dp"
                android:textColor="#283241"
                android:textColorHint="#495057"
                android:textSize="17dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.559"
                tools:ignore="Autofill,HardcodedText,KeyboardInaccessibleWidget,LabelFor,RtlCompat,RtlHardcoded,RtlSymmetry,SpUsage,UnknownIdInLayout,UseCompatTextViewDrawableXml" />

            <TextView
                android:layout_width="353dp"
                android:layout_height="19dp"
                android:paddingStart="15dp"
                android:text="Hospital of Origin"
                android:textSize="12dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.613"
                tools:ignore="HardcodedText,RtlSymmetry,SpUsage" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="355dp"
                android:layout_height="19dp"
                android:paddingStart="15dp"
                android:text="Specialized in"
                android:textSize="12dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.718"
                tools:ignore="HardcodedText,RtlSymmetry,SpUsage" />

            <Button
                android:id="@+id/btnGuardarContacto"
                android:layout_width="179dp"
                android:layout_height="46dp"
                android:layout_below="@+id/listaConversion"
                android:background="@drawable/button"
                android:text="Save"
                android:textAlignment="center"
                android:textAllCaps="false"
                android:textColor="#fff"
                android:textSize="40px"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.887"
                tools:ignore="HardcodedText" />

            <Spinner
                android:id="@+id/listaConversion"
                android:layout_width="353dp"
                android:layout_height="40dp"
                android:layout_below="@+id/textView2"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:background="@drawable/edit_text_background"
                android:outlineAmbientShadowColor="#fff"
                android:popupBackground="#ededed"
                android:spinnerMode="dropdown"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.668"
                tools:ignore="RtlHardcoded,UnknownId" />

            <Spinner
                android:id="@+id/listaConversion2"
                android:layout_width="353dp"
                android:layout_height="40dp"
                android:layout_below="@+id/textView2"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:background="@drawable/edit_text_background"
                android:outlineAmbientShadowColor="#fff"
                android:popupBackground="#ededed"
                android:spinnerMode="dropdown"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.774"
                tools:ignore="RtlHardcoded,UnknownId,UnknownIdInLayout" />

        </androidx.constraintlayout.widget.ConstraintLayout>


    </RelativeLayout>



</androidx.constraintlayout.widget.ConstraintLayout>